{
  "parameterDefinitions": [
    {
      "name": "ProjectFile",
      "type": "PATH",
      "objectType": "FILE",
      "dataFlow": "IN",
      "userInterface": {
        "control": "CHOOSE_INPUT_FILE",
        "label": "Project file",
        "groupLabel": "Source",
        "fileFilters": [
          {
            "label": "After Effects project files",
            "patterns": [
              "*.aep",
              "*.aepx"
            ]
          },
          {
            "label": "All Files",
            "patterns": [
              "*"
            ]
          }
        ]
      },
      "description": "The After Effects project file to render."
    },
    {
      "name": "RenderQueueIndex",
      "type": "INT",
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Render Queue Index",
        "groupLabel": "Source"
      },
      "description": "The index of the item in the render queue to render.",
      "default": 1
    },
    {
      "name": "OutputDir",
      "type": "PATH",
      "objectType": "DIRECTORY",
      "dataFlow": "OUT",
      "userInterface": {
        "control": "CHOOSE_DIRECTORY",
        "label": "Output Directory",
        "groupLabel": "Output"
      },
      "description": "The directory to write the output files to."
    },
    {
      "name": "OutputFileName",
      "type": "STRING",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Output File Name",
        "groupLabel": "Output"
      },
      "description": "The file name to use for the output images (e.g. comp_[#####].png)."
    },
    {
      "name": "Frames",
      "type": "STRING",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Frame Range",
        "groupLabel": "Output"
      },
      "description": "The frame range to render (e.g. 0-100)."
    },
    {
      "name": "IgnoreMissingDependencies",
      "type": "STRING",
      "default": "OFF",
      "allowedValues": [
        "ON",
        "OFF"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Ignore missing dependencies",
        "groupLabel": "Ignore Missing Dependencies"
      },
      "description": "Allows render to continue without failing if referenced files are missing."
    },
    {
      "name": "Concurrency",
      "type": "INT",
      "default": -1,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "STMPO Concurrency (-1=auto)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "MaxConcurrency",
      "type": "INT",
      "default": 24,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "STMPO Max Concurrency",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "RamPerProcessGB",
      "type": "FLOAT",
      "default": 32.0,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "RAM per aerender (GB)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "DisableMFR",
      "type": "STRING",
      "default": "false",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Disable Multi-Frame Rendering (MFR)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "DisableAffinity",
      "type": "STRING",
      "default": "false",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Disable CPU affinity / NUMA pinning",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "SpawnDelay",
      "type": "FLOAT",
      "default": 2.0,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Spawn delay (sec)",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "ChildGraceSec",
      "type": "INT",
      "default": 10,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Child grace before kill (sec)",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "NoKillOnFail",
      "type": "STRING",
      "default": "false",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Do NOT kill siblings on first failure",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "OutputIsPattern",
      "type": "STRING",
      "default": "false",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Output is already a pattern",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "AerenderPath",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Custom aerender.exe path (optional)",
        "groupLabel": "Advanced Paths"
      }
    },
    {
      "name": "EnvFile",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Env JSON (job-attached relative or absolute)",
        "groupLabel": "Advanced Paths"
      }
    },
    {
      "name": "LogFile",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "STMPO log file path (optional)",
        "groupLabel": "Advanced Paths"
      }
    }
  ]
}
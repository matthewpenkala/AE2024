{
  "parameterDefinitions": [
    {
      "name": "ProjectFile",
      "type": "PATH",
      "objectType": "FILE",
      "dataFlow": "IN",
      "userInterface": {
        "control": "CHOOSE_INPUT_FILE",
        "label": "Project file",
        "groupLabel": "Source",
        "fileFilters": [
          {
            "label": "After Effects project files",
            "patterns": [
              "*.aep",
              "*.aepx"
            ]
          }
        ]
      }
    },
    {
      "name": "CompName",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Comp name (optional)",
        "groupLabel": "Source"
      }
    },
    {
      "name": "RenderQueueIndex",
      "type": "INT",
      "default": 1,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Render Queue Index",
        "groupLabel": "Source"
      }
    },
    {
      "name": "OutputPath",
      "type": "PATH",
      "objectType": "FILE",
      "dataFlow": "OUT",
      "userInterface": {
        "control": "CHOOSE_OUTPUT_FILE",
        "label": "Output path (image sequence)",
        "groupLabel": "Output"
      }
    },
    {
      "name": "FrameRange",
      "type": "STRING",
      "default": "0-100",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "Frame range (start-end)",
        "groupLabel": "Frame Range"
      }
    },
    {
      "name": "ChunkSize",
      "type": "INT",
      "default": 0,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Chunk size (0=full range)",
        "groupLabel": "Frame Range"
      }
    },
    {
      "name": "STMPOConcurrency",
      "type": "INT",
      "default": -1,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "STMPO Concurrency (-1=auto)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "MaxConcurrency",
      "type": "INT",
      "default": 24,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "STMPO Max Concurrency",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "RamPerProcessGB",
      "type": "FLOAT",
      "default": 32.0,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "RAM per aerender (GB)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "DisableMFR",
      "type": "STRING",
      "default": "false",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Disable Multi-Frame Rendering (MFR)",
        "groupLabel": "STMPO"
      }
    },
    {
      "name": "DisableAffinity",
      "type": "STRING",
      "default": "true",
      "allowedValues": [
        "true",
        "false"
      ],
      "userInterface": {
        "control": "CHECK_BOX",
        "label": "Disable CPU affinity",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "SpawnDelay",
      "type": "FLOAT",
      "default": 2.0,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Spawn delay (sec)",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "ChildGraceSec",
      "type": "INT",
      "default": 10,
      "userInterface": {
        "control": "SPIN_BOX",
        "label": "Child grace before kill (sec)",
        "groupLabel": "STMPO Advanced"
      }
    },
    {
      "name": "EnvFile",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "STMPO env override JSON (optional)",
        "groupLabel": "Advanced Paths"
      }
    },
    {
      "name": "LogFile",
      "type": "STRING",
      "default": "",
      "userInterface": {
        "control": "LINE_EDIT",
        "label": "STMPO log file path (optional)",
        "groupLabel": "Advanced Paths"
      }
    }
  ]
}